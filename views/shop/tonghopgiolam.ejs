<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/orders.css">
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <ul class="orders">
                <% for (let prod of inf) { %> 
                <li class="orders__item">
                    <h1>Ngày làm việc: <%= prod.dayWork %></h1>
                    <h1>Tổng số giờ làm: </h1>
                    <h1>Annualeave: </h1>
                    <ul class="orders__products">
                        <li class="orders__products-item">Giờ vào: <%= prod.startDate %></li>
                        <li class="orders__products-item">Giờ kết thúc: <%= prod.endDate %></li>
                        <li class="orders__products-item">Địa điểm làm việc: <%= prod.place %></li>
                        <li class="orders__products-item">Số giờ tính làm thêm (sau 8h): </li>
                    </ul>
                </li>
                <% } %>
            </ul>    
        </main>
        <%- include('../includes/end.ejs') %>