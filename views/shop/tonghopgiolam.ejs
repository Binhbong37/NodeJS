<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/orders.css">
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <table>
                    <h2>Danh sách giờ làm</h2>
                    <tr>
                      <th>Ngày</th>
                      <th>Giờ vào</th>
                      <th>Giờ ra</th>
                      <th>Địa điểm</th>
                      <th>Giờ làm thêm</th>
                    </tr>
                    <% for (let prod of inf) { %> 
                    <tr>
                      <td><%= prod.dayWork %></td>
                      <td><%= prod.startDate %></td>
                      <td><%= prod.endDate %></td>
                      <td><%= prod.place %></td>
                      <td><%= prod.overTime %> giờ</td>
                    </tr>
                    <% } %>
            </table>
            <table>
                <h2>Xin nghỉ phép</h2>
                <p></p>
            </table>
        </main>
        <%- include('../includes/end.ejs') %>